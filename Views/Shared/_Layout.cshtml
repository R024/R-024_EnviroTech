<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top" style="text-align: center;">

        <img src="~/Content/othLogos.png" height="90px" style="    margin-top: 10px;" />
    </div>
    <div class="container body-content" style="margin-top: 34px;">
        @RenderBody()
        <hr />

    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
    <footer style="text-align:center;color:black; width: 100%;height:23px;bottom: 0;left: 0;background-color: #00A896;">
       Enviro. Tech. 'Team R-024'
    </footer>
</body>

</html>


